{
  "private": true,
  "version": "0.1.0-alpha.4",
  "scripts": {
    "release": "bumpp package.json packages/*/package.json --all --execute \"tsx build/bump-php-packages.ts\" --commit \"release: v\"",
    "build": "pnpm -r build",
    "test": "vitest --run",
    "test:all": "pnpm run test && composer test",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "eslint \"**/*.{ts,vue,mjs}\" --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "ci": "pnpm run lint && pnpm run build && pnpm run test"
  },
  "packageManager": "pnpm@7.13.4",
  "devDependencies": {
    "@fontsource/rubik": "^4.5.14",
    "@hybridly/config": "workspace:*",
    "@hybridly/core": "workspace:*",
    "@hybridly/progress-plugin": "workspace:*",
    "@hybridly/utils": "workspace:*",
    "@hybridly/vite": "workspace:*",
    "@hybridly/vue": "workspace:*",
    "@innocenzi/eslint-config": "^0.16.1",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.14.2",
    "@types/qs": "^6.9.7",
    "@types/throttle-debounce": "^5.0.0",
    "@vitest/ui": "^0.29.2",
    "axios": "^1.3.4",
    "bumpp": "^9.0.0",
    "eslint": "^8.35.0",
    "happy-dom": "^8.9.0",
    "hybridly": "workspace:*",
    "msw": "^1.1.0",
    "shiki": "^0.14.1",
    "shiki-processor": "^0.1.3",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "unbuild": "1.1.2",
    "unocss": "^0.50.1",
    "vite": "^4.1.4",
    "vitepress": "1.0.0-alpha.49",
    "vitest": "^0.29.2",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "overrides": {
      "unbuild": "0.8.11"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
