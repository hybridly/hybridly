{
  "name": "@hybridly/utils",
  "type": "module",
  "version": "0.7.8",
  "description": "Utils used in Hybridly packages",
  "author": "Enzo Innocenzi <enzo@innocenzi.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/innocenzi",
  "homepage": "https://github.com/hybridly/hybridly/tree/main/packages/config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hybridly/hybridly.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/hybridly/hybridly/issues"
  },
  "keywords": [
    "hybridly"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "debug": "^4.3.5",
    "deepmerge": "^4.3.1",
    "is-plain-object": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9"
  }
}
